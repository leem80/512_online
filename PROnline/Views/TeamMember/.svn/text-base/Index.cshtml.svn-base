@model IEnumerable<PROnline.Models.Teams.TeamMember>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_2Section.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Level
        </th>
        <th>
            Declaration
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Level)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Declaration)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TeamMemberID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TeamMemberID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TeamMemberID })
        </td>
    </tr>
}

</table>
